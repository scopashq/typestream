{
  "name": "@typestream/sdk",
  "version": "0.0.9",
  "description": "SDK for the rapid development of data transformation projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/scopashq/typestream",
    "path": "packages/sdk"
  },
  "license": "MIT",
  "author": "Scopas Technologies GmbH",
  "type": "module",
  "bin": {
    "tyst": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "check-deps": "depcheck --ignores oclif,@oclif/*,@types/dotenv",
    "clean": "rimraf dist/ node_modules/",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.1",
    "@google-cloud/storage": "^5.18.2",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@supercharge/promise-pool": "^2.1.0",
    "@types/dotenv-safe": "^8.1.2",
    "@typestream/core-protocol": "^0.0.5",
    "ansi-escapes": "^5.0.0",
    "chalk": "^5.0.0",
    "chalk-template": "^0.4.0",
    "chokidar": "^3.5.3",
    "cli-cursor": "^4.0.0",
    "compare-versions": "^4.1.3",
    "date-fns": "^2.28.0",
    "dotenv-safe": "^8.2.0",
    "esbuild": "^0.14.18",
    "genson-js": "^0.0.8",
    "json-schema-to-typescript": "^10.1.5",
    "ora": "^6.1.0",
    "p-defer": "^4.0.0",
    "prompts": "^2.4.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "16",
    "@types/prompts": "^2.0.14",
    "oclif": "^2",
    "typescript": "^4.7.0-dev.20220216"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "tyst",
    "dirname": "typestream-sdk",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./dist/hooks/init/assert-node-version"
    }
  }
}
